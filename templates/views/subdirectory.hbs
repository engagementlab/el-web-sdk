<section class="main-container padding-bottom-clear">
	<div class="container">
		<div class="main">
			<h1 class="page-title"><strong>{{upperCase section}}: </strong><span class="font-light">{{name}}</span></h1>
			{{> page/lead}}
		</div>
	</div>

	<div class="container pv-20">
		<div class="row">
			{{#if listings}}
				
				{{!-- Show projects --}}
				{{#each listings}}
					{{> sublisting listing=this}}
				{{/each}}

			{{else}}
				
				<strong>No projects! Fiddlesticks!</strong>
			
			{{/if}}
		</div>
	</div>
</section>
